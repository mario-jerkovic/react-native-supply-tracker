{
  "compilerOptions": {
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": [
      "es6",
      "dom"
    ],
    "module": "es6",
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitAny": true,
    "paths": {
      "*": [
        "node_modules/*",
        "src/typings/*"
      ]
    },
    "target": "es6"
  },
  "exclude": [
    "node_modules"
  ]
}
